<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="di.test01.MyCalculator" id ="mycalculator"/>
	<bean class="di.test01.Calculator" id = "calculator"/>

	<bean class="di.test01.MyCalculator" id="MyCal">
		<property name="calculator" ref="calculator"></property>
		<property name="firstNum" value="10"></property>
		<property name="secondNum" value="3"></property>
	</bean>
</beans>